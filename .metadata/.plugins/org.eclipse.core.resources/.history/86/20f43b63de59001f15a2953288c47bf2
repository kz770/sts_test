package com.example.demo.util;

import com.example.demo.vo.EmpVO;

public class EmpUtil {
	public String getHtmlEmp(EmpVO vo) {
		String text="";
		text+="<td>vo.getEname()</td>";
		text+="<td>vo.getJob()</td>";
		text+="<td>vo.getSalary()</td>";
		text+=vo.getEmail();
		text+=vo.getJumin();
		text+=vo.getComm();
		text+=vo.getEno();
		text+=vo.getDno();
		text+=vo.getHiredate();
		text+=vo.getMgr();
		return text;
	}
}
